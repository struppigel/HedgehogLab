        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00// 01 02 03/* 04*/</Keywords>
            <Keywords name="Numbers, prefix1">a b c d e f A B C D E F (</Keywords>
            <Keywords name="Numbers, prefix2">0x</Keywords>
